source 'https://cdn.cocoapods.org/'
source 'https://github.com/ShuStuff/ShuSpecs.git'

use_frameworks!
platform :ios, '10.0'
supports_swift_versions '>= 5.0'

target 'Shu' do
    ## Main business modules
    pod 'ShuAudioPlayer', '~> 0.1.0'

    ## Third party libs
    pod 'Alamofire'
    pod 'SnapKit'
    pod 'Kingfisher'

    ### For debug
    pod 'LookinServer', :configurations => ['Debug']
end